<div class="artists-list">
  {{#each filteredArtistsArr}}
    <div class="single-item">
      <img src="{{images.1.url}}" alt="" />
      <h4>{{this.name}}</h4>
      <a class="view-link" href="/albums/{{this.id}}">View Albums</a>
    </div>
  {{/each}}
</div>
<div class="pagination-buttons">
  <a href="">PREVIOUS</a>
  <span>Page {{currentPage}}</span>
  <a href="{{this.linkNext}}">NEXT</a>
</div>