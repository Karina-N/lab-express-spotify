<h2 class="albums-header">Albums for: {{this.artist}}</h2>
<div class="albums-list">
  {{#each albumsArray}}
    <div class="single-item">
      <img src="{{this.images.1.url}}" alt="" />
      <h3>{{this.name}}</h3>
      <a class="view-link" href="/albums/artist/{{this.id}}">View Tracks</a>
    </div>
  {{/each}}
</div>